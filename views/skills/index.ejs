<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Express Dev-Skills</title>
</head>
<body>
  
  <h1>Skills as of <%= time %></h1>
  
  <ul>
    <% skills.forEach(function(skill) { %>
      <li>
        <a href="/skills/<%= skill._id %>">
				<%= skill.text %></a>
				- 
				<%= skill.done ? 'done' : 'not done' %>
			</li>
    <% }) %>
  </ul>
  <a href="/skills/new">Skills List</a>
</body>
</html>